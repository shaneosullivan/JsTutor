{
  "version": 2,
  "buildCommand": "npm run build",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.0.0",
      "includeFiles": ["dist/public/**", "vite.config.ts", "shared/**", "server/**"]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}